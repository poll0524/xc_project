<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.collecttoolserver.mapper.vipOrederMapper">
    <insert id="insertOrder" parameterType="com.example.collecttoolserver.pojo.VipOreder">
        insert into vip_order (open_id,order_number,order_money,order_time,pay_type) values (#{openId},#{orderNumber},#{orderMoney},#{orderTime},#{payType});
    </insert>
</mapper>