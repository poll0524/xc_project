<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.xcschoolserver.mapper.reasonMapper">
    <select id="selectReason" resultType="com.example.xcschoolserver.pojo.Reason">
        select * from reason where start = #{start};
    </select>

    <select id="selectId" resultType="com.example.xcschoolserver.pojo.Reason">
        select * from reason where reason_id = #{reasonId};
    </select>
</mapper>